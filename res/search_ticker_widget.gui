@column{
	layout{
		dx {max}
		dy {max}
	}
	@row{
		layout{
			dx {max}
		}
		@text_input_line{
			id {query_text_input}

			text {"enter query here"}

			layout{
				dx {0}
				dy {min}
				weight {1}
			}
		}
		@push_button{
			id {query_push_button}
			@text{
				text {"run query"}
			}
		}
	}
	@list{
		id {tickers_list}
		layout{
			dy {0}
			dx {fill}
			weight {1}
		}
	}
}
@busy{
	id {busy_spinner}
}
