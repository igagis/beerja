@row{
	@scroll_area{
		layout{
			dy {fill}
			dx {100dp}
			weight {1}
		}
		@color{
			layout{
				dx {100}
				dy {100}
			}

			color {0xff00ffff}
		}
		u_cells_container{
			layout{
				dx {max}
				dy {min}
			}
		}
	}

	@color{
		layout{
			dx {1dp}
			dy {fill}
		}
		color {0xff808080}
	}

	@widget{
		id{search_ticker_stub}
		layout{
			dx { 300dp }
			dy {max}
		}
	}
}
