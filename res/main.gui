@overlay{
	@row{
		lp{
			dx {max}
			dy {max}
		}
		@scroll_area{
			lp{
				dy {fill}
				dx {100dp}
				weight {1}
			}
			@color{
				lp{
					dx {100}
					dy {100}
				}

				color {0xff00ffff}
			}
			u_cells_container{
				lp{
					dx {max}
					dy {min}
				}
			}
		}

		@color{
			lp{
				dx {1dp}
				dy {fill}
			}
			color {0xff808080}
		}

		@widget{
			id{search_ticker_stub}
			lp{
				dx { 300dp }
				dy {max}
			}
		}
	}
}
